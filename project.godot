[application]

config/name="Finder"
run/main_scene="res://Title.tscn"
boot_splash/image="res://Avatar.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://resources/images/logo.png"

[autoload]

Info="*res://scripts/Info.gd"

[display]

window/size/width=480
window/size/height=854
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_clear_color=Color( 0.843137, 0.85098, 0.74902, 1 )
environment/default_environment="res://resources/default_env.tres"
